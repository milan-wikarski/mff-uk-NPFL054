# Returns probability table
probability <- function(x) {
  return(table(x) / length(x))
}
# Returns the entropy of a given feature
entropy <- function(x) {
  p <- probability(x)
  return (-sum(p * log2(p)))
}
entropy(data$x)
entropy(data$y)
